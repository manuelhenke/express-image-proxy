{
  "name": "express-image-proxy",
  "version": "1.0.0",
  "description": "Simple express application for proxy images to adjust head fields for example. ",
  "private": true,
  "main": "index.js",
  "scripts": {
    "build": "npx tsc",
    "start": "node dist/index.js",
    "dev": "concurrently \"npx tsc --watch\" \"nodemon -q dist/index.js\"",
    "lint": "eslint --ext \".ts\" --ignore-path .gitignore .",
    "lintfix": "yarn lint --fix",
    "prepare": "husky install",
    "test": "echo \"Error: no test specified\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/manuelhenke/express-image-proxy.git"
  },
  "license": "MIT",
  "author": {
    "name": "Manuel Henke",
    "email": "contact@henkebyte.com",
    "url": "https://henkebyte.com/"
  },
  "bugs": {
    "url": "https://github.com/manuelhenke/express-image-proxy/issues"
  },
  "homepage": "https://github.com/manuelhenke/express-image-proxy#readme",
  "dependencies": {
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "http-status-codes": "^2.2.0",
    "request": "^2.88.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.4",
    "@commitlint/config-conventional": "^17.4.4",
    "@types/express": "^4.17.17",
    "@types/node": "^18.14.6",
    "@types/request": "^2.48.8",
    "@typescript-eslint/eslint-plugin": "^5.54.0",
    "@typescript-eslint/parser": "^5.57.0",
    "concurrently": "^7.6.0",
    "eslint": "^8.35.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-sonarjs": "^0.18.0",
    "husky": "^8.0.3",
    "nodemon": "^2.0.21",
    "prettier": "^2.8.4",
    "typescript": "^4.9.5"
  }
}
